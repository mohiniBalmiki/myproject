# TaxWise Frontend Environment Variables
# Copy this file to .env.local and fill in your actual values

# API Configuration
VITE_API_BASE_URL=http://localhost:5000/api
VITE_BACKEND_URL=http://localhost:5000

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# App Configuration
VITE_APP_NAME=TaxWise
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=AI-Powered Tax Management Platform

# Feature Flags
VITE_ENABLE_DEMO_MODE=true
VITE_ENABLE_FILE_UPLOAD=true
VITE_ENABLE_TAX_CALCULATOR=true
VITE_ENABLE_CIBIL_ANALYSIS=true

# Debug Configuration
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info

# AI Services Configuration
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_ML_API_ENDPOINT=http://localhost:5000/api/ml